<h1>活動記録</h1>
<%= form_with model: [@team,@diary], url:team_diaries_path do |f| %>
  <h4>投稿チーム</h4>
  <%#= f.collection_select :team_id, TeamUser.where(user_id: current_user.id), :id, :name, include_blank: "選択してください" %>
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>
  <h4>タイトル</h4>
  <%= f.text_field :title %>
  <h4>活動内容</h4>
  <%= f.text_area :body %>
  <%= f.submit '投稿' %>
<% end %>