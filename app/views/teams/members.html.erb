            <% @members.each do |member| %>
              <%= link_to member.user.name, user_path(member.user) %>
              <% if current_user.id == @team.owner_id && member.id != @team.owner_id %>
                <%=link_to "追放する", team_banishment_path(@team, team_user_id: member.id), method: :delete, class: "btn btn-sm btn-danger" %>
              <% end %>
            <% end %>
